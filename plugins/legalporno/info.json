{
  "name": "legalporno",
  "version": "0.1.0",
  "authors": ["boi123212321"],
  "description": "Extract Legalporno shoot IDs (e.g. GIO482) from video names",
  "pluginEvents": ["sceneCreated", "sceneCustom"],
  "arguments": [
    {
      "name": "deep",
      "type": "Boolean",
      "required": false,
      "default": true,
      "description": "Fetch scene details"
    },
    {
      "name": "dry",
      "type": "Boolean",
      "required": false,
      "default": false,
      "description": "Whether to commit data changes"
    },
    {
      "name": "setName",
      "type": "Boolean",
      "required": false,
      "default": false,
      "description": "Whether to set scene name to found shoot ID"
    }
  ]
}
